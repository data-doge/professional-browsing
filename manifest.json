{
  "manifest_version" : 2,
  "name" : "professional browsing",
  "version" : "1.0",
  "description" : "browsing for professionals",
  "content_scripts" : [
    {
      "all_frames": true,
      "matches" : ["http://*/*", "https://*/*"],
      "js" : ["jquery-2.1.3.min.js", "content.js"]
    }
  ],
  "permissions": ["<all_urls>"],
  "browser_action" : {
    "default_icon" : "pentagram.png",
    "default_popup" : "popup.html"
  }
}